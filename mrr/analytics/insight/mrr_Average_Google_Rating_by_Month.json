{
  "$id": "mrr_Average_Google_Rating_by_Month",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Average Google Rating by Month",
  "description": "Average Monthly Rating on Google",
  "query": {
    "metrics": [
      "yext/AVERAGE_RATING"
    ],
    "dimensions": [
      "yext/MONTHS"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "MONTHS_12"
      },
      {
        "filter": "yext/PARTNERS",
        "in": [
          "715"
        ]
      }
    ],
    "comparison": "PREVIOUS_YEAR"
  },
  "display": {
    "line": {
      "showLabels": true
    }
  }
}