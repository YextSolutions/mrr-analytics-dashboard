{
  "$id": "mrr_Yext_MRR_Responses",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Yext MRR Responses",
  "description": "Responses posted by your Yext MRR team.",
  "query": {
    "metrics": [
      "yext/RESPONSE_COUNT"
    ],
    "dimensions": [
      "yext/PARTNERS"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "MONTHS_12"
      }
    ]
  },
  "display": {
    "hero": {
      "aggregation": "SUM"
    }
  }
}