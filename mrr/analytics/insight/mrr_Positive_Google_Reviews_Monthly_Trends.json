{
  "$id": "mrr_Positive_Google_Reviews_Monthly_Trends",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Positive Google Reviews Monthly Trends",
  "description": "5-star monthly reviews on Google",
  "query": {
    "metrics": [
      "yext/NEW_REVIEWS"
    ],
    "dimensions": [
      "yext/MONTHS"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "MONTHS_12"
      },
      {
        "filter": "yext/PARTNERS",
        "in": [
          "715"
        ]
      },
      {
        "filter": "yext/RATINGS",
        "in": [
          "5",
          "4"
        ]
      }
    ],
    "comparison": "PREVIOUS_YEAR"
  },
  "display": {
    "line": {
      "showLabels": true
    }
  }
}