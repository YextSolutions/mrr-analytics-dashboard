{
  "$id": "mrr_Reviews_by_Month_and_Site",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Reviews by Month and Site",
  "description": "Reviews by Month and Site",
  "query": {
    "metrics": [
      "yext/NEW_REVIEWS"
    ],
    "dimensions": [
      "yext/MONTHS",
      "yext/PARTNERS"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "MONTHS_12"
      }
    ]
  },
  "display": {
    "bar": {
      "showLabels": true,
      "style": "PERCENT"
    }
  }
}