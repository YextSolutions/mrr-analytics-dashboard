{
  "$id": "mrr_Response_Rate_by_Month",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Response Rate by Month",
  "description": "Response Rate by Month",
  "query": {
    "metrics": [
      "yext/REVIEW_RESPONSE_RATE"
    ],
    "dimensions": [
      "yext/MONTHS"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "MONTHS_12"
      }
    ]
  },
  "display": {
    "bar": {
      "showLabels": true
    }
  }
}