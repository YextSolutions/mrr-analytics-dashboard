{
  "$id": "mrr_Google_Reviews_by_Ratings_YoY",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Google Reviews by Ratings YoY",
  "description": "Google ratings distribution and year over year growth",
  "query": {
    "metrics": [
      "yext/NEW_REVIEWS"
    ],
    "dimensions": [
      "yext/RATINGS"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "MONTHS_12"
      },
      {
        "filter": "yext/PARTNERS",
        "in": [
          "715"
        ]
      }
    ],
    "comparison": "PREVIOUS_YEAR"
  },
  "display": {
    "bar": {
      "showLabels": true
    }
  }
}