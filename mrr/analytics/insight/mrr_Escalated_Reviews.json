{
  "$id": "mrr_Escalated_Reviews",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Escalated Reviews",
  "description": "High-risk reviews escalated by Yext MRR.",
  "query": {
    "metrics": [
      "yext/NEW_REVIEWS"
    ],
    "dimensions": [
      "yext/PARTNERS"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "MONTHS_12"
      }
    ]
  },
  "display": {
    "hero": {
      "aggregation": "SUM"
    }
  }
}