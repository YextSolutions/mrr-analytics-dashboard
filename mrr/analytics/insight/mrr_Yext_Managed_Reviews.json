{
  "$id": "mrr_Yext_Managed_Reviews",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Yext Managed Reviews",
  "description": "Total reviews managed by Yext MRR.\nIncludes responses, escalations, and flags.",
  "query": {
    "metrics": [
      "yext/NEW_REVIEWS"
    ],
    "dimensions": [
      "yext/PARTNERS"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "MONTHS_12"
      }
    ]
  },
  "display": {
    "hero": {
      "aggregation": "SUM"
    }
  }
}