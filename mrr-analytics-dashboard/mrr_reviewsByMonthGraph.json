{
    "$id": "mrr_reviewsByMonthGraph",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Reviews by Month",
    "description": "Reviews by Month",
    "query": {
      "metrics": [
        "yext/NEW_REVIEWS"
      ],
      "dimensions": [
        "yext/MONTHS"
      ],
      "filters": [
        {
          "filter": "yext/date",
          "in": "MONTHS_12"
        },
        {
          "filter": "yext/PARTNERS",
          "in": [
            "559",
            "715",
            "439"
          ]
        }
      ],
      "comparison": "PREVIOUS_YEAR"
    },
    "display": {
      "line": {
        "showLabels": true
      }
    }
  }