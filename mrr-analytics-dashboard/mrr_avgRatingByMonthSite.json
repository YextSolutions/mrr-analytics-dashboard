{
    "$id": "mrr_avgRatingByMonthSite",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Average Rating by Month (GMB)",
    "description": "Average Rating by Month (GMB Only)",
    "query": {
      "metrics": [
        "yext/AVERAGE_RATING"
      ],
      "dimensions": [
        "yext/MONTHS",
        "yext/PARTNERS"
      ],
      "filters": [
        {
          "filter": "yext/date",
          "in": "MONTHS_12"
        },
        {
          "filter": "yext/partners",
          "in": [
            "715,713,558"
          ]
        }
      ]
    },
    "display": {
      "line": {
        "showLabels": true
      }
    }
  }