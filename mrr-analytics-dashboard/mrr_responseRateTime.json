{
    "$id": "mrr_responseRateTime",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Response Rate and Response Time (Hours) by Month",
    "description": "Response Rate and Response Time (Hours) by Month",
    "query": {
      "metrics": [
        "yext/REVIEW_RESPONSE_RATE",
        "yext/REVIEW_RESPONSE_TIME"
      ],
      "dimensions": [
        "yext/MONTHS"
      ],
      "filters": [
        {
          "filter": "yext/date",
          "in": "MONTHS_12"
        },
        {
          "filter": "yext/PARTNERS",
          "in": [
            "559",
            "715",
            "439"
          ]
        }
      ]
    },
    "display": {
      "bar": {
        "showLabels": true
      }
    }
  }