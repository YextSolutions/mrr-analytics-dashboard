{
    "$id": "mrr_avgRatingByMonth",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Average Rating by Month",
    "description": "Average Rating by Month",
    "query": {
      "metrics": [
        "yext/AVERAGE_RATING"
      ],
      "dimensions": [
        "yext/MONTHS"
      ],
      "filters": [
        {
          "filter": "yext/date",
          "in": "MONTHS_12"
        }
      ]
    },
    "display": {
      "line": {}
    }
  }